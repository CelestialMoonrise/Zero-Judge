/******************************************************************************

d490. 我也愛偶數

*******************************************************************************/
#include <iostream>

using namespace std;

int main()
{
    int total, a, b;
    cin >> a >> b;
    
    for(int i=a; i<=b; i++){
        if(i%2==0){
            total+=i;
        }
    }
    
    cout << total;

    return 0;
}
